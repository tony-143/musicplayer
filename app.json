{
  "expo": {
    "name": "BMusic",
    "slug": "Music",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "WAKE_LOCK",
        "FOREGROUND_SERVICE"
      ],
      "package": "com.yourname.musicapp",
      "playback": {
        "isForeground": true
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}